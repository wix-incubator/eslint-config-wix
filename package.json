{
  "name": "eslint-config-wix",
  "version": "1.0.0",
  "description": "Shared eslint config for all Wix projects",
  "scripts": {
    "build": ":",
    "lint": "eslint . --ignore-pattern 'test/scripts'",
    "pretest": "npm run lint -s",
    "test": "mocha --reporter mocha-env-reporter",
    "release": "wnpm-release",
    "postpublish": "rm -f npm-shrinkwrap.json"
  },
  "files": [
    "index.js",
    "esnext.js",
    "angular.js",
    "react.js",
    "mocha.js",
    "react-native.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-private/eslint-config-wix.git"
  },
  "author": "Shahar Talmi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-private/eslint-config-wix/issues"
  },
  "homepage": "https://github.com/wix-private/eslint-config-wix#readme",
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "dependencies": {
    "eslint-config-xo": "~0.14.0",
    "eslint-config-xo-react": "~0.7.0",
    "eslint-plugin-babel": "~3.2.0",
    "eslint-plugin-react": "~5.0.1",
    "babel-eslint": "latest",
    "eslint-plugin-mocha": "~2.2.0",
    "eslint-plugin-react-native-wix": "^1.0.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "~2.9.0",
    "mocha": "~2.5.3",
    "mocha-env-reporter": "~1.0.2",
    "wnpm-ci": "latest"
  },
  "peerDependencies": {
    "eslint": ">=2.9.0"
  }
}
